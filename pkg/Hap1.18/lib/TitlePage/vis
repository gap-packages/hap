
vi -c "%s/<Item>/<Br\/>/g"  -c "wq" "$@" 

vi -c "%s/<Row>/<Br\/>/g"  -c "wq" "$@"

vi -c "%s/<\/Item>/<Br\/>/g"  -c "wq" "$@"

vi -c "%s/<\/Row>/<Br\/>/g"  -c "wq" "$@"

vi -c "%s/<Table Align=\"|l|\" >/<Br\/>/g"  -c "wq" "$@"

vi -c "%s/<\/Table>/<Br\/>/g"  -c "wq" "$@"

vi -c "%s/<List>/<Br\/>/g"  -c "wq" "$@"

vi -c "%s/<\/List>/<Br\/>/g"  -c "wq" "$@"

vi -c "%s/<\/C>/<\/C><Br\/>/g"  -c "wq" "$@"

vi -c "%s/<\/Index>/<\/Index>::::::::::::::::::::::::<Br\/>/g"  -c "wq" "$@"




