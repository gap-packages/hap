Q:=ConnectedQuandle(24,17,"import");;
K:=PureCubicalKnot(3,1);;
L:=ReflectedCubicalKnot(K);;
square:=KnotSum(K,L);;
granny:=KnotSum(K,K);;
gcsquare:=GaussCodeOfPureCubicalKnot(square);;
gcgranny:=GaussCodeOfPureCubicalKnot(granny);;
Qsquare:=PresentationKnotQuandle(gcsquare);;
Qgranny:=PresentationKnotQuandle(gcgranny);;
Print(NumberOfHomomorphisms(Qsquare,Q),"\n");
Print(NumberOfHomomorphisms(Qgranny,Q),"\n");
